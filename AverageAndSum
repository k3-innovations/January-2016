package EXercise1;

public class AddAverage {
	
	
	public static void main(String[] args) {
		int[] number = new int[]{1,2,3,4,5};
		int total=0;
	for(int x: number) {
		  total = total + x;
		  
		  
		
	}
	
	double ave = total/number.length;
	
	int average = total/number.length;
	System.out.println(total);
	System.out.println(ave);
	}

	}
